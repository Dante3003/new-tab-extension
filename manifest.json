{
  "manifest_version": 3,
  "version": "1.0",
  "name": "New Tab",
  "description": "Default new tab take over with incredible background",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "128": "assets/icon.png"
  },
  "action": {
    "default_icon": "assets/icon.png"
  },
  "permissions": ["storage", "unlimitedStorage"]
}
